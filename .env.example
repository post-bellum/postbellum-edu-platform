# .env.example
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Schools registry data URL (Czech Ministry of Education) - REQUIRED
SCHOOLS_REGISTRY_URL=

NEXT_PUBLIC_QA_EMAILS=qa@example.com,test@qa.example.com
NEXT_PUBLIC_QA_OTP_CODE=111111

# Optional: Set to enable browser password protection (shows native login prompt)
# PASSWORD_PROTECTION_PASSWORD=your-secret-password

# Optional: Set to enable coming soon mode (redirects all routes to /)
# COMING_SOON_MODE=false

# Optional: Umami analytics - only set in production
# NEXT_PUBLIC_UMAMI_WEBSITE_ID=

